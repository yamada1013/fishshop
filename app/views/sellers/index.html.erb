<h1>Sellers#index</h1>
<p>Find me in app/views/sellers/index.html.erb</p>

<% @sellers.each do |seller| %>
  <%=  link_to seller_path(seller) do %>
    <%= seller.sellername %>
  <% end %>
  <%= link_to seller_path(seller) do %>
    <%= attachment_image_tag seller, :profile_image , fallback: "no-image.png" %>
  <% end%>
<% end %>
